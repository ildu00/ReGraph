{
  "name": "regraph",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for ReGraph - Decentralized AI Compute Marketplace",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "regraph",
    "ai",
    "machine-learning",
    "inference",
    "openai",
    "llm",
    "gpt",
    "claude",
    "llama",
    "decentralized",
    "compute",
    "typescript"
  ],
  "author": "ReGraph Team <support@regraph.tech>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ildu00/ReGraph.git",
    "directory": "sdk/javascript"
  },
  "bugs": {
    "url": "https://github.com/ildu00/ReGraph/issues"
  },
  "homepage": "https://regraph.tech",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
